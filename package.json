{
  "name": "custom-gifs-slack-command",
  "version": "0.0.1",
  "private": true,
  "description": "Sample Slack app that responds to an Slack slash command with an interactive message",
  "license": "UNLICENSED",
  "author": "Pez Cuckow",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:write": "yarn run format:write:js && prettier --write .",
    "format:write:js": "eslint --fix .",
    "lint": "yarn run lint:js && prettier --check .",
    "lint:js": "eslint .",
    "start": "DEBUG=slash-command-template node src/index.js",
    "start:dev": "nodemon src/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.js": "eslint --cache --fix",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fuse.js": "^6.4.6",
    "glob": "^7.1.6",
    "glob-promise": "^4.1.0",
    "synonyms": "^1.0.1",
    "tsscmp": "^1.0.6",
    "weighted-random": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.49.0"
  },
  "engines": {
    "node": ">=14.15.0",
    "yarn": ">=1.22.10"
  },
  "authors": [
    "Pez Cuckow <email@pezcuckow.com>"
  ]
}
