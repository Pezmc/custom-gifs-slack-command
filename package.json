  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.2",
    "debug-level": "^2.1.2",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "fuse.js": "^6.6.2",
    "glob": "^7.2.3",
    "glob-promise": "^4.2.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "tsscmp": "^1.0.6",
    "weighted-random": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.5.1"
  },
  "engines": {
    "node": "^16.18.0",
    "yarn": "^1.22.10"
  },
  "volta": {
    "node": "16.18.0"
  },
  "authors": [
    "Pez Cuckow <email@pezcuckow.com>"
  ]
